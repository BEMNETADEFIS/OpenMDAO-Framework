#!/usr/local/bin/python
import sys
    
sys.path[0:0] = [
  '/home/bnaylor/dev/OpenMDAO/release/eggs/openmdao.main-0.1-py2.5.egg',
  '/usr/local/lib/python2.5/site-packages/setuptools-0.6c8-py2.5.egg',
  ]

_interactive = True
if len(sys.argv) > 1:
    import getopt
    _options, _args = getopt.getopt(sys.argv[1:], 'ic:')
    _interactive = False
    for (_opt, _val) in _options:
        if _opt == '-i':
            _interactive = True
        elif _opt == '-c':
            exec _val
            
    if _args:
        sys.argv[:] = _args
        execfile(sys.argv[0])

if _interactive:
    import code
    code.interact(banner="", local=globals())
