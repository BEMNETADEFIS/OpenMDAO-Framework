#!/usr/local/bin/python
import sys
    
sys.path[0:0] = [
  '/home/bnaylor/OpenMDAO/developers/bnaylor/T001-my_feature/openmdao.main',
  '/home/bnaylor/OpenMDAO/developers/bnaylor/T001-my_feature/buildout/eggs/conmin-1.0-py2.5-linux-x86_64.egg',
  '/home/bnaylor/OpenMDAO/developers/bnaylor/T001-my_feature/buildout/eggs/numpy-1.2.1-py2.5-linux-x86_64.egg',
  '/home/bnaylor/OpenMDAO/developers/bnaylor/T001-my_feature/buildout/eggs/pyparsing-1.5.1-py2.5.egg',
  '/home/bnaylor/OpenMDAO/developers/bnaylor/T001-my_feature/buildout/eggs/setuptools-0.6c9-py2.5.egg',
  ]

_interactive = True
if len(sys.argv) > 1:
    import getopt
    _options, _args = getopt.getopt(sys.argv[1:], 'ic:')
    _interactive = False
    for (_opt, _val) in _options:
        if _opt == '-i':
            _interactive = True
        elif _opt == '-c':
            exec _val
            
    if _args:
        sys.argv[:] = _args
        execfile(sys.argv[0])

if _interactive:
    import code
    code.interact(banner="", local=globals())
