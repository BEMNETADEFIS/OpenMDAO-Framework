[
    { "text": "Project",
      "items": [
        { "text": "Save",          "onclick": "openmdao.model.saveProject();" },
        { "text": "Run",           "onclick": "openmdao.model.runModel();" },
        { "text": "Reload",        "onclick": "openmdao.model.reload();" },
        { "text": "Close",         "onclick": "openmdao.model.close();" },
        { "text": "Exit",          "onclick": "openmdao.model.exit();" }
      ]
    },
    { "text": "View",
      "items": [
        { "text": "Code Editor",   "onclick": "jQuery('#code_tab').click();" },
        { "text": "Command Line",  "onclick": "openmdao.Util.toggle_visibility('cmdform');" },
        { "text": "Console",       "onclick": "layout.open('south');" },
        { "text": "Files",         "onclick": "layout.open('west'); jQuery('#ftree_tab').click();" },
        { "text": "Libraries",     "onclick": "layout.open('east'); jQuery('#palette_tab').click();" },
        { "text": "Objects",       "onclick": "layout.open('west'); jQuery('#otree_tab').click();" },
        { "text": "Properties",    "onclick": "layout.open('east'); jQuery('#properties_tab').click();" },
        { "text": "Workflow",      "onclick": "jQuery('#workflow_tab').click();" },
        { "text": "Dataflow",      "onclick": "jQuery('#dataflow_tab').click();" },
        { "text": "--------"       },
        { "text": "Refresh",       "onclick": "openmdao.update();" }
      ]
    },
    { "text": "Tools",
      "items": [
        { "text": "Plot",          "onclick": "openmdao.PlotFrame.prototype.chooseVariable();"},
        { "text": "Plugins",       "onclick": "openmdao.Util.popupWindow('addons','Addons',575,540);"}
      ]
    },
    { "text": "Help",
      "items": [
        { "text": "Documentation", "onclick": "openmdao.Util.popupWindow('http://openmdao.org/docs','',768,1024);"}
      ]
    },
    { "text": "About",             "onclick": "openmdao.Util.popupWindow('http://openmdao.org/','',768,1024);"}
]
